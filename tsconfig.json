{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": false,
        "module": "esnext",
        "target": "es5",
        "allowJs": true,
        "moduleResolution": "node",
        "types": [
            "element-plus/global"
        ],
        "baseUrl": "./src/",
        "skipLibCheck": true,
        "paths": {
            "components": [
                "components"
            ],
            "pages": [
                "pages"
            ],
            "config": [
                "config"
            ],
            "types": [
                "types"
            ],
            "store": [
                "store"
            ]
        }
    }
}